// BATER√çA DE PRUEBAS COMPLETA
// ============================================

function testCompleto() {
  Logger.log('‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó');
  Logger.log('‚ïë     CODEHUBBER v2.3 - BATER√çA DE PRUEBAS COMPLETA         ‚ïë');
  Logger.log('‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù');
  Logger.log('');
  
  let testsPasados = 0;
  let testsFallados = 0;
  
  // ============================================
  // TEST 1: OBTENER PROYECTOS
  // ============================================
  Logger.log('üìã TEST 1: obtenerProyectos()');
  try {
    const proyectos = obtenerProyectos();
    Logger.log('  ‚úÖ PAS√ì - Proyectos obtenidos: ' + proyectos.length);
    if (proyectos.length > 0) {
      Logger.log('  üìä Primer proyecto: ' + proyectos[0].nombre);
    }
    testsPasados++;
  } catch (error) {
    Logger.log('  ‚ùå FALL√ì - ' + error.message);
    testsFallados++;
  }
  Logger.log('');
  
  // ============================================
  // TEST 2: OBTENER PROYECTO ESPEC√çFICO
  // ============================================
  Logger.log('üîç TEST 2: obtenerProyecto(rowIndex)');
  try {
    const proyectos = obtenerProyectos();
    if (proyectos.length > 0) {
      const proyecto = obtenerProyecto(proyectos[0].rowIndex);
      Logger.log('  ‚úÖ PAS√ì - Proyecto: ' + proyecto.nombre);
      Logger.log('  üìä RowIndex: ' + proyecto.rowIndex);
      testsPasados++;
    } else {
      Logger.log('  ‚ö†Ô∏è SALTADO - No hay proyectos para probar');
    }
  } catch (error) {
    Logger.log('  ‚ùå FALL√ì - ' + error.message);
    testsFallados++;
  }
  Logger.log('');
  
  // ============================================
  // TEST 3: CREAR PROYECTO TEMPORAL
  // ============================================
  Logger.log('‚ûï TEST 3: crearProyecto(nombre)');
  let proyectoTestRowIndex = null;
  try {
    const proyectosAntes = obtenerProyectos().length;
    const resultado = crearProyecto('TEST_TEMPORAL_' + new Date().getTime());
    const proyectosDespues = obtenerProyectos().length;
    
    if (proyectosDespues === proyectosAntes + 1) {
      Logger.log('  ‚úÖ PAS√ì - Proyecto creado correctamente');
      proyectoTestRowIndex = resultado[resultado.length - 1].rowIndex;
      Logger.log('  üìä RowIndex del proyecto test: ' + proyectoTestRowIndex);
      testsPasados++;
    } else {
      throw new Error('El n√∫mero de proyectos no aument√≥');
    }
  } catch (error) {
    Logger.log('  ‚ùå FALL√ì - ' + error.message);
    testsFallados++;
  }
  Logger.log('');
  
  // ============================================
  // TEST 4: ACTUALIZAR CAMPO
  // ============================================
  Logger.log('‚úèÔ∏è TEST 4: actualizarCampo(rowIndex, campo, valor)');
  try {
    if (proyectoTestRowIndex) {
      const valorTest = 'Info de prueba - ' + new Date().toLocaleString();
      const resultado = actualizarCampo(proyectoTestRowIndex, 'info', valorTest);
      
      if (resultado.info === valorTest) {
        Logger.log('  ‚úÖ PAS√ì - Campo actualizado correctamente');
        Logger.log('  üìä Valor guardado: ' + resultado.info.substring(0, 30) + '...');
        testsPasados++;
      } else {
        throw new Error('El valor no se guard√≥ correctamente');
      }
    } else {
      Logger.log('  ‚ö†Ô∏è SALTADO - No hay proyecto de prueba');
    }
  } catch (error) {
    Logger.log('  ‚ùå FALL√ì - ' + error.message);
    testsFallados++;
  }
  Logger.log('');
  
  // ============================================
  // TEST 5: REORDENAR PROYECTO
  // ============================================
  Logger.log('üîÑ TEST 5: reordenarProyecto(rowIndex, nuevoOrden)');
  try {
    if (proyectoTestRowIndex) {
      const ordenAntes = obtenerProyecto(proyectoTestRowIndex).orden;
      reordenarProyecto(proyectoTestRowIndex, 1);
      const ordenDespues = obtenerProyecto(proyectoTestRowIndex).orden;
      
      if (ordenDespues === 1) {
        Logger.log('  ‚úÖ PAS√ì - Proyecto reordenado correctamente');
        Logger.log('  üìä Orden antes: ' + ordenAntes + ', despu√©s: ' + ordenDespues);
        testsPasados++;
      } else {
        throw new Error('El orden no cambi√≥ como se esperaba');
      }
    } else {
      Logger.log('  ‚ö†Ô∏è SALTADO - No hay proyecto de prueba');
    }
  } catch (error) {
    Logger.log('  ‚ùå FALL√ì - ' + error.message);
    testsFallados++;
  }
  Logger.log('');
  
  // ============================================
  // TEST 6: API DE GITHUB - EXTRAER INFO URL
  // ============================================
  Logger.log('üîó TEST 6: extraerInfoGitHub(url)');
  try {
    const testUrl = 'https://github.com/koki81pe/CodeHubber/tree/main';
    const info = extraerInfoGitHub(testUrl);
    
    if (info.user === 'koki81pe' && info.repo === 'CodeHubber' && info.branch === 'main') {
      Logger.log('  ‚úÖ PAS√ì - Informaci√≥n extra√≠da correctamente');
      Logger.log('  üìä User: ' + info.user + ', Repo: ' + info.repo + ', Branch: ' + info.branch);
      testsPasados++;
    } else {
      throw new Error('La informaci√≥n extra√≠da no es correcta');
    }
  } catch (error) {
    Logger.log('  ‚ùå FALL√ì - ' + error.message);
    testsFallados++;
  }
  Logger.log('');
  
  // ============================================
  // TEST 7: API DE GITHUB - GENERAR LINKLIST
  // ============================================
  Logger.log('üåê TEST 7: generarLinkListDesdeTree(treeUrl)');
  try {
    const testUrl = 'https://github.com/koki81pe/CodeHubber/tree/main';
    const resultado = generarLinkListDesdeTree(testUrl);
    
    if (resultado.success && resultado.totalArchivos > 0) {
      Logger.log('  ‚úÖ PAS√ì - LinkList generado correctamente');
      Logger.log('  üìä Total archivos: ' + resultado.totalArchivos);
      Logger.log('  üìä Primeros 200 chars del LinkList:');
      Logger.log('  ' + resultado.linkList.substring(0, 200) + '...');
      testsPasados++;
    } else {
      throw new Error('No se gener√≥ el LinkList correctamente');
    }
  } catch (error) {
    Logger.log('  ‚ùå FALL√ì - ' + error.message);
    testsFallados++;
  }
  Logger.log('');
  
  // ============================================
  // TEST 8: ELIMINAR PROYECTO TEMPORAL
  // ============================================
  Logger.log('üóëÔ∏è TEST 8: eliminarProyecto(rowIndex)');
  try {
    if (proyectoTestRowIndex) {
      const proyectosAntes = obtenerProyectos().length;
      eliminarProyecto(proyectoTestRowIndex);
      const proyectosDespues = obtenerProyectos().length;
      
      if (proyectosDespues === proyectosAntes - 1) {
        Logger.log('  ‚úÖ PAS√ì - Proyecto eliminado correctamente');
        Logger.log('  üìä Proyectos antes: ' + proyectosAntes + ', despu√©s: ' + proyectosDespues);
        testsPasados++;
      } else {
        throw new Error('El proyecto no se elimin√≥ correctamente');
      }
    } else {
      Logger.log('  ‚ö†Ô∏è SALTADO - No hay proyecto de prueba para eliminar');
    }
  } catch (error) {
    Logger.log('  ‚ùå FALL√ì - ' + error.message);
    testsFallados++;
  }
  Logger.log('');
  
  // ============================================
  // RESUMEN
  // ============================================
  Logger.log('');
  Logger.log('‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó');
  Logger.log('‚ïë                    RESUMEN DE PRUEBAS                      ‚ïë');
  Logger.log('‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù');
  Logger.log('');
  Logger.log('‚úÖ Tests pasados: ' + testsPasados);
  Logger.log('‚ùå Tests fallados: ' + testsFallados);
  Logger.log('üìä Total de tests: ' + (testsPasados + testsFallados));
  Logger.log('');
  
  if (testsFallados === 0) {
    Logger.log('üéâüéâüéâ ¬°TODOS LOS TESTS PASARON! üéâüéâüéâ');
  } else {
    Logger.log('‚ö†Ô∏è Algunos tests fallaron. Revisa los errores arriba.');
  }
  
  Logger.log('');
  Logger.log('‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê');
}


